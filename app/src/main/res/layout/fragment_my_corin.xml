<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/corin_lightGray"
    tools:context=".corinDomain.domain.mainActivity.coronaInfoFragment.CoronaInfoFragment">

    <com.example.corincoronacheckincustomer.jshCrossDomain.JSHToolBar
        android:id="@+id/jshToolbar"
        style="@style/jshToolBar"
        app:jshToolBar_title="@string/myCorinFragment_toolBarTitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/myCorinFragment_loginLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/jshToolbar">

        <TextView
            android:id="@+id/myCorinFragment_loginLayout_infoTextView"
            style="@style/corinContentSubTitleTextView"
            android:layout_marginTop="24dp"
            android:text="@string/myCorinFragment_loginLayout_infoTextViewText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/myCorinFragment_loginLayout_loginButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="8dp"
            android:text="@string/myCorinFragment_loginLayout_loginButtonText"
            app:layout_constraintEnd_toStartOf="@+id/myCorinFragment_loginLayout_signUpButton"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="spread"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/myCorinFragment_loginLayout_infoTextView" />

        <Button
            android:id="@+id/myCorinFragment_loginLayout_signUpButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:backgroundTint="@color/white"
            android:text="@string/myCorinFragment_loginLayout_signUpButtonText"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/myCorinFragment_loginLayout_loginButton"
            app:layout_constraintTop_toBottomOf="@+id/myCorinFragment_loginLayout_infoTextView" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.example.corincoronacheckincustomer.jshCrossDomain.JSHSelectItem
        android:id="@+id/myCorinFragment_editMyInfoSelectItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:jshSelectItem_title="@string/myCorinFragment_editMyInfoSelectItemText"
        app:layout_constraintTop_toBottomOf="@+id/myCorinFragment_loginLayout" />

    <com.example.corincoronacheckincustomer.jshCrossDomain.JSHSelectItem
        android:id="@+id/myCorinFragment_aboutCorinSelectItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:jshSelectItem_title="@string/myCorinFragment_aboutCorinSelectItemText"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/myCorinFragment_editMyInfoSelectItem" />

    <com.example.corincoronacheckincustomer.jshCrossDomain.JSHSelectItem
        android:id="@+id/myCorinFragment_aboutServiceSelectItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        app:jshSelectItem_title="@string/myCorinFragment_aboutServiceSelectItemText"
        app:layout_constraintTop_toBottomOf="@+id/myCorinFragment_aboutCorinSelectItem" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/myCorinFragment_loginAndSignOutLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myCorinFragment_aboutServiceSelectItem">

        <TextView
            android:id="@+id/myCorinFragment_loginAndSignOutLayout_logOutTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:text="@string/myCorinFragment_loginAndSignOutLayout_logOutTextViewText"
            app:layout_constraintBottom_toBottomOf="@+id/myCorinFragment_loginAndSignOutLayout_sighOutTextView"
            app:layout_constraintEnd_toStartOf="@+id/myCorinFragment_loginAndSignOutLayout_verticalDivider1"
            app:layout_constraintTop_toTopOf="@+id/myCorinFragment_loginAndSignOutLayout_sighOutTextView" />

        <View
            android:id="@+id/myCorinFragment_loginAndSignOutLayout_verticalDivider1"
            style="@style/verticalDivider"
            android:layout_height="10dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/myCorinFragment_loginAndSignOutLayout_sighOutTextView"
            app:layout_constraintEnd_toStartOf="@+id/myCorinFragment_loginAndSignOutLayout_sighOutTextView"
            app:layout_constraintTop_toTopOf="@+id/myCorinFragment_loginAndSignOutLayout_sighOutTextView" />

        <TextView
            android:id="@+id/myCorinFragment_loginAndSignOutLayout_sighOutTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/myCorinFragment_loginAndSignOutLayout_sighOutTextView"
            android:layout_marginHorizontal="16dp"
            android:layout_marginVertical="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>