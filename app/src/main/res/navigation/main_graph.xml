<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/checkInFragment">

    <fragment
        android:id="@+id/coronaInfoFragment"
        android:name="com.example.corincoronacheckincustomer.CoronaInfoFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_corona_info" />
    <fragment
        android:id="@+id/checkInFragment"
        android:name="com.example.corincoronacheckincustomer.CheckInFragment"
        android:label="fragment_check_in"
        tools:layout="@layout/fragment_check_in" />
    <fragment
        android:id="@+id/myCorinFragment"
        android:name="com.example.corincoronacheckincustomer.MyCorinFragment"
        android:label="fragment_my_corin"
        tools:layout="@layout/fragment_my_corin" />
    <action android:id="@+id/action_global_coronaInfoFragment" app:destination="@id/coronaInfoFragment" />
    <action android:id="@+id/action_global_checkInFragment" app:destination="@id/checkInFragment" />
    <action android:id="@+id/action_global_myCorinFragment" app:destination="@id/myCorinFragment" />
</navigation>